<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
  version="2.5" metadata-complete="true">
  
  <servlet>
    <servlet-name>BotServlet</servlet-name>
    <servlet-class>com.poi.server.servlets.BotServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>BotServlet</servlet-name>
    <url-pattern>/bot</url-pattern>
  </servlet-mapping> 
  
  <servlet>
    <servlet-name>PoiProximityServlet</servlet-name>
    <servlet-class>com.poi.server.servlets.PoiProximityServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PoiProximityServlet</servlet-name>
    <url-pattern>/poiproximity</url-pattern>
  </servlet-mapping> 
  
</web-app>
